NOTE THE CODE IS IN THE JAVA





class Solution {
    static int totalRooms(int n, int m, int k ) {
    // code here
    int count=0;
    if(n/(k+0.0)==n/k)
    {
        count+=n/k;
    }
    else
    {
        count+=n/k+1;
    }
    if(m/(k+0.0)==m/k)
    {
        count+=m/k;
    }
    else
    {
        count+=m/k+1;
    }
    return count;
    
    
  }
}






