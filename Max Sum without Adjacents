class Solution{

public: 

 // calculate the maximum sum with out adjacent

 int findMaxSum(int *arr, int n) {

     int dp[n+1];

     dp[0] = 0;

     dp[1] = arr[0];

     for(int i=2; i<n+1; i++){

         dp[i] = max(dp[i-2]+arr[i-1], dp[i-1]);

     }

     return dp[n];

 }

};





